import random

# list of possible characters
chars = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789!@#$%^&*()"

# length of the password
length = 16

password = ""

# create the password
for i in range(length):
    password += random.choice(chars)

# print the password
print(password)
